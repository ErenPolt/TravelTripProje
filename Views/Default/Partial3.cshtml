@using TravelTripProje.Models.Sınıflar
@model List<Blog>


<div class="padding">
    <h4>Görmek İstediğim Şehirler</h4>
    <ul class="list list-pad">
    @{ int i = 0;}
        @foreach (var item in Model)
        {
            <li class="list-bg"><span>@{i++;}@i.</span><a href="/Blog/BlogDetay/@item.Id">@item.Title</a></li>

        } </ul>
    <a href="/Blog/Index/" class="link1">Blog Sayfasına Git→</a>
    </div> 